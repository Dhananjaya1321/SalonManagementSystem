	<queryString language="SQL">
		<![CDATA[select c.name,c.nic,bd.rent_id,bd.qty,bd.for_how_many_days,b.bok_id,b.date,r.name as rental_name
,r.Price_pre_day,r.Discount,(bd.for_how_many_days*(r.Price_pre_day-((r.Price_pre_day*r.Discount)/100)))
as total from customer c join book b on c.nic=b.nic join book_rentals_detail bd on 
b.bok_id = bd.bok_id join rentals r on r.Rent_Id = bd.rent_id  where b.bok_id = 
(SELECT bok_id FROM book ORDER BY bok_id DESC LIMIT 1) and b.status = 'Paid']]>
	</queryString>
