	<queryString language="SQL">
		<![CDATA[select c.name,c.nic,a.Apt_Id,a.date,a.time,s.sev_id,s.name as Service_name,
s.price,s.Discount,(s.price-((s.price*s.Discount)/100))
as total from customer c join appointment a on c.nic=a.nic join service_appointment_detail ad on 
ad.Apt_Id = a.Apt_Id join service s on s.Sev_Id = ad.Sev_Id  where a.Apt_Id = 
